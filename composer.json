{
    "name": "kimhf/advanced-custom-fields-pro-stubs",
    "description": "Stubs for the Advanced Custom Fields Pro WordPress plugin, used for static analysis.",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "composer",
            "url": "https://pivvenit.github.io/acf-composer-bridge/composer/v3/wordpress-plugin/"
        }
    ],
    "keywords": [
        "wordpress",
        "advanced custom fields pro",
        "static analysis"
    ],
    "support": {
        "issues": "https://github.com/kimhf/advanced-custom-fields-stubs-pro/issues",
        "source": "https://github.com/kimhf/advanced-custom-fields-stubs-pro"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kim Helge Frimanslund",
            "homepage": "https://github.com/kimhf"
        }
    ],
    "require-dev": {
        "php": ">=7.1",
        "giacocorsiglia/stubs-generator": "^0.5",
        "advanced-custom-fields/advanced-custom-fields-pro": "5.10.0"
    },
    "scripts": {
        "generate-stubs": "generate-stubs --finder=advanced-custom-fields-pro-finder.php --nullify-globals --out=advanced-custom-fields-pro-stubs.php --force"
    },
    "extra": {
        "installer-paths": {
            "plugins/{$name}/": [
                "type:wordpress-plugin"
            ]
        }
    }
}
